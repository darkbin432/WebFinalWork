<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hznu.lwb.persistence.ProjectDao">

    <select id="getOne" parameterType="int" resultType="com.hznu.lwb.model.Project">
        select * from project where id = #{id} and status=1;
    </select>

    <select id="selectAll" resultType="Project">
        select * from project where status=1
    </select>

</mapper>
